@charset "utf-8";
html{width: 100%;height: 100%;}
body{
	padding: 0;
	margin: 0 auto;
	height: 100%;
	background:#f2f2f2;
	font-size:16px;
	font-family: Helvetica,STHeiti STXihei, Microsoft JhengHei, Microsoft YaHei, Arial;
	color: #000;
	-webkit-text-size-adjust:100%;
	-webkit-user-select: none;
	user-select: none;
}
div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,form,input,p,th,td {margin:0; padding:0;font-family: Helvetica,STHeiti STXihei, Microsoft JhengHei, Microsoft YaHei, Arial;}
h1,h2,h3,h4,h5,h6{font-size:100%; font-weight:normal;}
ul,li,ol{list-style:none;}
img{border:none;}
a{text-decoration: none;outline:none}
a:hover{color:#000;}
.ui-page-theme-a a{font-weight:normal}
em{display: block;clear: both;}
table{border-collapse:collapse; border-spacing:0;}
input{vertical-align:middle;}
.fl{float:left;}
.fr{float:right;}
.tl{ text-align:left;}
.tc{ text-align: center;}
.tr{ text-align: right;}
.pr{ position:relative;}
.pa{ position:absolute;}
.ui-page-theme-a{text-shadow:none;}
#wrapper {
    margin: 0 auto;
    max-width: 640px;
    overflow: hidden;
    font-size: 14px;
	position:relative;
}
.header{ background:#172327;position: relative;}
.header h1 {padding: 5px 10px;}
.header h1 img {height:2.875em; display:block;}
.claim_box{
	border-top:1.25em solid #f2f2f2;
	padding:0 1em 1em;
}
.claim_box h2{
	font-size:1.5em;
	line-height:2em;
	margin-bottom:0.5em;
	padding:5px 0;
	border-bottom:1px solid #e1e1e1;
}
.claim_box p{
	margin:5px 0;
	line-height: 1.5em;
	font-size:1.25em;
	word-break:break-all;
}
.sui-btn{
	margin-top:1.5em;
	padding:.5em 0;
	border-top:1px solid #d7d7d7;
}
.sui-btn a:first-child{margin-right:1em;}
.sui-btn a:active{background:#58aded;}
.sui-btn .ui-btn,.sui-btn .ui-btn-cancel{
	font-weight:normal;
	padding:1em;
	border:0;
	-webkit-border-radius: .3125em;
    border-radius: .3125em;
	margin:0;
	vertical-align:middle;
	font-size:1em;
	background:#f0572c !important;
	color:#fff !important;
	text-shadow:none;
	display:inline-block;
	padding:.675em;
	width:5em;
}
.sui-btn .ui-btn-cancel{background:#f2f2f2;color:#000;	}
.sui-btn .ui-btn:hover,.sui-btn .ui-btn:active{background:#58aded; color:#fff;text-shadow: 0 0 0 #f3f3f3;text-shadow:none;}
.claim_error{padding:5em .5em 0; font-size:1.25em; line-height:1em;}
.error_info p{margin-bottom:5px;}
.ui-dialog-contain {
	width: 92.5%;
	max-width: 500px;
	margin: 25% auto 15px auto;
	padding: 0;
	position: relative;
}


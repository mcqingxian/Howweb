html,
body {
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100% !important;
    /* background: url("../../images/wechat/partner.png") center center/cover no-repeat; */
}
.map-open{
	height:100%;
}
/* .container {
    width: 100%;
    
    position: relative;
    height: 100%;
    overflow: hidden;
    background: url("../../images/wechat/partner.png") center center/cover no-repeat;
} */
.container {
    width: 100%;
    /* margin-top:50px; */
    position: absolute;
    background: url("../../images/wechat/partner.png") center bottom/100% 100% no-repeat;
}
.ui-page{
	height:inherit !important;
}
.wrapper-in {
    height: 100%;
}
.main-page {
    height: 100%;
}
.wrapper{
	height:inherit;
}

.footer{
	display: none;
}

.btn_box {
    position: absolute;
    bottom: 45px;
    left: 25%;
    width: 50%;
}
.detailOne,
.detailTwo {
    position: absolute;
    width: 91px;
    height: 16px;
    background: url("../../images/wechat/bg_btn01.png") center bottom/100% 100% no-repeat;
    /* background-color: rgb(232, 77, 13); */
    font-size: 8px;
    line-height: 16px;
    text-align: center;
    border-radius: 6px;
    color: #fff;
    left: 22%;
}

.detailOne {
    top: 16%;
}

.detailTwo {
    top: 31%;
}
#btn {
    width: 100%;
    border: none;
    outline: none;
    /* line-height: 38px; */
    font-size: 16px;
    color: #fff;
    background: url("../../images/wechat/partner01.png") no-repeat;
    background-size: 100% 100%;
    display: inline-block;
    text-align: center;
    padding: 8px;
    box-sizing: border-box;
}

@media screen and (max-width:320px) {
    .detailOne {
        left: 23%;
        top: 16%;
    }
    .detailTwo {
        left: 23%;
        top: 31%;
    }
}
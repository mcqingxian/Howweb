package com.hoau.how.module.bse.api.shared.exception;

import com.hoau.hbdp.framework.exception.BusinessException;

/**
 * 留言信息异常类
 *
 * @author 张爱萍
 * @date 2015年5月31日
 */
public class CommentException extends BusinessException {
	
	
	/**
	 * 分页参数为空
	 */
	public static final String RB_NULL = "how.bse_api_module.comment.RbNullException";
	
	/**
	 * 留言信息为空
	 */
	public static final String COMMENT_NULL = "how.bse_api_module.comment.CommentException";

	/**
	  * 功能异常类定义
	  * @param errCode
	  * @since
	 */
	public CommentException(String errCode){
		super();
		super.errCode = errCode;
	}
	public CommentException(String errCode, Object... para) {
		super(errCode, para);
		super.errCode = errCode;
	}

}

package com.hoau.wechat.vo;

public class LotteryActivityDraw {
	// 1 中间 0 未中奖
	private int status;
	// 奖券
	private VoucherActivity voucherActivity;
	// 备注
	private String remark;
	// 是否有抽奖资格 0 无资格
	private boolean qualification;
	// 图片名称
	private String imageName;
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public VoucherActivity getVoucherActivity() {
		return voucherActivity;
	}
	public void setVoucherActivity(VoucherActivity voucherActivity) {
		this.voucherActivity = voucherActivity;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public boolean isQualification() {
		return qualification;
	}
	public void setQualification(boolean qualification) {
		this.qualification = qualification;
	}
	public String getImageName() {
		return imageName;
	}
	public void setImageName(String imageName) {
		this.imageName = imageName;
	}
	
}
